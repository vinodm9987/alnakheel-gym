{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/server/index.js",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outputCapture": "std"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "url": "http://localhost:5001",
      "webRoot": "${workspaceFolder}"
    }
  ]
}